{
  "expo": {
    "name": "Machakos Hospital",
    "slug": "MachakosHospitalMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "machakoshospitalmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "android": {
      "package": "com.kennedymutuku.machakoshospitalmobile",
      "icon": "./assets/images/icon.png",
      "edgeToEdgeEnabled": true
    },

    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png"
    },

    "web": {
      "favicon": "./assets/images/icon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "e0a2378b-3efa-4bc7-89a2-46d366ca7224"
      }
    }
  }
}
